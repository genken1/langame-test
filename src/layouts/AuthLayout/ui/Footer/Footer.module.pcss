.footer {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 19px;

    &Links {
        display: flex;
        gap: 16px;
    }

    &Company {
        display: flex;
        align-items: center;

        &FirstPart {
            font-family: "Lato Regular", sans-serif;
            font-size: 15px;
        }

        &SecondPart {
            font-family: "Lato Black", sans-serif;
            font-size: 15px;
        }
    }
}
